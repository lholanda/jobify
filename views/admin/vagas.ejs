<%- include('header.ejs') %>
  <h2>
    Admin Vagas:
    <a href="/admin/vagas/nova"><button>Nova Vaga</button></a>
    <a href="/"><button>Home</button></a>
  </h2>
  <div>
    <ul>
      <% vagas.forEach( vaga => { %> 
        <li>
          <%- vaga.titulo %> -
          <%- vaga.categoria %> |  
          <a href="/admin/vagas/delete/<%- vaga.id %>">Excluir</a>
          <a href="/admin/vagas/editar/<%- vaga.id %>">Editar</a>
        </li>
      <% }) %>
    </ul>
  </div>
 <%- include('footer.ejs') %>